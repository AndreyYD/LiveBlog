 (function(rootElem) {
  var discText = rootElem.querySelector('.js-trg-item-disclamer-text');
  if (discText) {
   discText.innerHTML = discText.innerHTML.replace(/<br\/?>/g, ' ');
  }
 })(document.getElementById("trg-b-105946611-455015142"));
</script>


</div>

