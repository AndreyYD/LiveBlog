    #b105019853_1413539271 .trg-b-close-wrap._hidden .trg-b-close-text {
      visibility: visible !important;
      display: block !important;
    }
    #b105019853_1413539271 .trg-b-close-wrap._hidden .trg-b-close-overlay {
      visibility: visible !important;
      display: block !important;
      background: #fff !important;
      opacity: 1;
    }
    #b105019853_1413539271 .trg-b-close-wrap._hidden .trg-b-close-cross,
    #b105019853_1413539271 .trg-b-close-wrap._hidden .trg-b-close-bubble {
