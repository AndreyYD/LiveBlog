<script nonce="">
	userKeeper.time('biz-app-load');

	__appBundleReady__.on(function runApp(mode) {
		// Запуск приложения
		userKeeper.time('app-run');

		_timers.init_app = -_now();
		var startTs = Date.now();

		// Конфигурация проекта
		define('cfg', ['config'], function (config) {
			sendLoadlogRadar('loadflow_step-23', 'cfg defined');
			var cfg = config({
  "port": 443,
  "auth": "account",
  "custom-backend": false,
  "static-http-path": "https://img.imgsmail.ru/octavius/68336/",
  "qa": false,
  "local-themes-version": "1.45.1",
  "deploy-ts": "68336",
  "is-production-build": "1",
  "build": "release-fmail-16557.1970-01-01T18_58_56",
  "production": true,
  "sota": true,
  "canary": false,
  "serverTime": 1648808461745
});

			var cfgOpts = sotaConfig.cfg;
			cfgOpts.urls.faviconURL = 'https://img.imgsmail.ru/octavius/68336/static/favicon/';

			cfg.set(cfgOpts);
			cfg.set('requirejs-config', {
				paths: requirejsConfig.paths,
			});

			return cfg;
		});

		define('cfgUser', ['config'], function (config) {
			var userCfg = sotaConfig.userConfig;

			return config(userCfg);
		});

		// Время загрузки основных модулей, перед загрузкой самого Приложения/app
		userKeeper.time('app-run-require');

		// Инициализация приложения
		require([
			'request',
			'RPC',
			'config',
			'setup',
			'cfg',
			'features',
			'@mail/flags',
			'mrg-xray',
			'cfgUser',
			
		], function (request, RPC, config, setup, cfg, features, flags, xray, cfgUser) {
			sendLoadlogRadar('loadflow_step-17', 'application init');
			features = features.default || features;

			userKeeper.timeEnd('app-run-require');

			// Вредя до завершения app-run, после которого начинается app-ready
			userKeeper.time('app-run-initialize');

			var initialize = function () {
				sendLoadlogRadar('loadflow_step-24', 'initialize called');

				flags.import(sotaConfig.flags);

				if (window.location.host === 'e.mail.ru') {
					if (features.has('turn-off-octavius')) {
						window.portalCookie.s.removeGlobal('octavius');

						appStarted('redirect');
						window.location.reload();
						return;
					}
				}

				var requirePaths = {};

					var themesKey = 'origin/release-fmail-16557:a046f5c'.split('/')
						.slice(-1)[0];
					var themesPathFeature = features.getDataOrDefault('themes-paths-v4', {});
					var themesPaths = themesPathFeature[themesKey] || themesPathFeature.default;

					cfg.themesPath = themesPaths.base;
					requirePaths.themesConfig = themesPaths.js + '/all.config';

						if (window.BROKEN_IMGS) {
							cfg.themesPath = getFallbackUrl(cfg.themesPath);
							requirePaths.themesConfig = getFallbackUrl(requirePaths.themesConfig);
						}


				var httpsRegexp = new RegExp('^https?://');
				Object.keys(requirePaths)
					.map(function (key) {
						requirePaths[key] = httpsRegexp.test(requirePaths[key]) ? requirePaths[key] : location.origin + requirePaths[key];
					});

				require.config({
					paths: requirePaths
				});

				// Предзагружаем конфиг тем
				require(['themesConfig']);

				// Мочим данные
				cfgUser.get('api')
					.forEach(function (item) {
						RPC.mock.once(item.method, item.data);

						if (item.method === '/api/v1/helpers') {
							flags.add(item.data && item.data.body);
						}

						if (item.method === '/api/v1/user/short') {
							cfg.themeId = item.data.body.theme;
							cfgUser.set('initialUser', item.data.body);
							cfgUser.set('initialUser.email', item.data.email);
						}
					});

				sendLoadlogRadar('loadflow_step-18', 'application run');

				userKeeper.timeEnd('app-run-initialize');
				userKeeper.timeEnd('app-run', {
					score: 'app:init',
					appendix: mode,
				});

				// Время потраченное на require(app)
				userKeeper.time('app-require');

				// Время загрузки от require(app) до app.ready.resolve
				userKeeper.time('app-ready');

				// FMAIL-15968 Фиксируем время, чтобы в слотах замерить сколько прошло до первого рендера рекламы
				if ('hidden' in document && !document.hidden) {  // исключаем фоновые вкладки
					_timers.ready_app = -_now();
				}

				require(['app', 'cfgUser'], function (/** Octavius */app) {
					userKeeper.timeEnd('app-require', {score: 'app:init'});

					sendLoadlogRadar('loadflow_step-25', 'app required');
					app = app.default;

					_timers.init_app += _now();

					app.ready(function () {
						userKeeper.timeEnd('app-ready', {score: 'app:init'});
						userKeeper.timeEnd('page-app-ready', {score: [5e3, 10e3, 15e3]});
						userKeeper.timeEnd('biz-app-ready', {score: 'app:ready'});
					});

					// Скрываем лоудер
					app.ready(window.__hideBootscreen__);
					app.on('login-page', window.__hideBootscreen__);

					_timers.data = -_now();
					window.app = app;

					// TODO: Удалить после проведения эксперимента https://jira.mail.ru/browse/FMAIL-13349
					if (mode) {
						xray.send('init-bundle_' + mode, {v: Date.now() - startTs, skipdwh: true});
					}

					sendLoadlogRadar('loadflow_step-19', 'runapp finish');
				});
			};

			// Инициализируем Feast
			sendLoadlogRadar('loadflow_step-26', 'feastInitialize');
			// Настройки feast по умолчанию
			require(['feast'], function (feast) {
				feast.set({
					// ref не будут попадать в DOM
					renderRefAttributes: false,
					useTryCatch: !features.has('disable-feast-try-catch')
				});
			});


				require(['feast'], function (feast) {
					// Добавляет обработчик аттрибутов "qa:id"
					feast.vdom.tags['@qa:id'] = function () {
						return false;
					};

					// Добавляет обработчик аттрибутов "qa:state"
					feast.vdom.tags['@qa:state'] = function () {
						return false;
					};

					initialize();
				});
		});

	});

	__appBundleLoad__();

	//# sourceURL=bottom-script.js
</script>

